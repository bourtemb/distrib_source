#!@SHELL@
applet_name=fr.esrf.logviewer.Main

if [ ! $TANGO_HOST ] && [ -f @TANGO_RC_FILE@ ]; then
   . @TANGO_RC_FILE@
fi

#
# Define the CLASSPATH
#
JAVALIB=@DATADIR@/java;
JAVABIN=@DATADIR@/java;
ATKBIN=@DATADIR@/java;
TLVBIN=@DATADIR@/java;

ATKJAR=$ATKBIN/ATKCore.jar:$ATKBIN/ATKWidget.jar:$ATKBIN/log4j.jar

CLASSPATH=$JAVALIB/TangORB.jar:$TLVBIN/LogViewer.jar:$ATKJAR:.
export CLASSPATH

@JAVA@ -DTANGO_HOST=$TANGO_HOST $applet_name $*
